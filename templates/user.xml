<templateSet group="user">
  <template name="ae" value="$ELEMENT$.addEventListener('$EVENT$', e =&gt; {&#10;    $BODY$&#10;});" description="DOM: addEventListener" toReformat="true" toShortenFQNames="true">
    <variable name="ELEMENT" expression="jsSuggestVariableName()" defaultValue="document" alwaysStopAt="true" />
    <variable name="EVENT" expression="jsDefineParameter()" defaultValue="" alwaysStopAt="true" />
    <variable name="BODY" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="qs" value="$ROOT_ELEMENT$.querySelector(`$SELECTOR$`)&#10;" description="DOM: querySelector" toReformat="true" toShortenFQNames="true">
    <variable name="ROOT_ELEMENT" expression="jsSuggestVariableName()" defaultValue="document" alwaysStopAt="true" />
    <variable name="SELECTOR" expression="jsDefineParameter()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="qsa" value="$ROOT_ELEMENT$.querySelectorAll(`$SELECTOR$`)&#10;" description="DOM: querySelectorAll" toReformat="true" toShortenFQNames="true">
    <variable name="ROOT_ELEMENT" expression="jsSuggestVariableName()" defaultValue="document" alwaysStopAt="true" />
    <variable name="SELECTOR" expression="jsDefineParameter()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
    </context>
  </template>
  <template name="clg" value="console.log(`${$VAR$}`);" description="JS: console.log" toReformat="true" toShortenFQNames="true">
    <variable name="VAR" expression="jsSuggestVariableName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>